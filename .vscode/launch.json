{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
        },
        {
            "name": "Python: Pytest",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "${workspaceFolder}/tests"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "HardPy: Build",
            "type": "debugpy",
            "request": "launch",
            "module": "build",
            "console": "integratedTerminal",
        },
        {
            "name": "HardPy: Init template",
            "type": "debugpy",
            "request": "launch",
            "module": "hardpy.cli.cli",
            "console": "integratedTerminal",
        },
        {
            "name": "HardPy: Run template",
            "type": "debugpy",
            "request": "launch",
            "module": "hardpy.cli.cli",
            "console": "integratedTerminal",
            "args": [
                "run",
                "template"
            ]
        },
        {
            "name": "StandCloud register",
            "type": "debugpy",
            "request": "launch",
            "module": "hardpy.cli.cli",
            "console": "integratedTerminal",
            "args": [
                "sc-register",
                "--no-verify-ssl",
                "examples/stand_cloud"
            ]
        },
        {
            "name": "StandCloud check connection",
            "type": "debugpy",
            "request": "launch",
            "module": "hardpy.cli.cli",
            "console": "integratedTerminal",
            "args": [
                "sc-register",
                "--no-verify-ssl",
                "--check",
                "examples/stand_cloud"
            ]
        },
        {
            "name": "Python: Example Hello HardPy",
            "type": "debugpy",
            "request": "launch",
            "module": "hardpy.cli.cli",
            "console": "integratedTerminal",
            "args": [
                "run",
                "examples/hello_hardpy"
            ]
        },
        {
            "name": "Python: Example CouchDB load",
            "type": "debugpy",
            "request": "launch",
            "module": "hardpy.cli.cli",
            "console": "integratedTerminal",
            "args": [
                "run",
                "examples/couchdb_load"
            ]
        },
        {
            "name": "Python: Example Minute parity",
            "type": "debugpy",
            "request": "launch",
            "module": "hardpy.cli.cli",
            "console": "integratedTerminal",
            "args": [
                "run",
                "examples/minute_parity"
            ]
        },
        {
            "name": "Python: Example StandCloud",
            "type": "debugpy",
            "request": "launch",
            "module": "hardpy.cli.cli",
            "console": "integratedTerminal",
            "args": [
                "run",
                "examples/stand_cloud"
            ]
        },
        {
            "name": "Python: Example Dialog box",
            "type": "debugpy",
            "request": "launch",
            "module": "hardpy.cli.cli",
            "console": "integratedTerminal",
            "args": [
                "run",
                "examples/dialog_box"
            ]
        },
        {
            "name": "Python: Example Attempts",
            "type": "debugpy",
            "request": "launch",
            "module": "hardpy.cli.cli",
            "console": "integratedTerminal",
            "args": [
                "run",
                "examples/attempts"
            ]
        },
        {
            "name": "Python: Example Operator Message",
            "type": "debugpy",
            "request": "launch",
            "module": "hardpy.cli.cli",
            "console": "integratedTerminal",
            "args": [
                "run",
                "examples/operator_msg"
            ]
        },
    ]
}
